<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="hibernate.table.MUser" table="m_user">

	<id name="userPid" column="user_pid"  >
		<generator class="assigned" />
	</id>
	
	<property name="name" column="name" length="50" type = "string"/>
	<property name="emailId" column="email_id" length="50" type = "string"/>
	<property name="contactNo" column="contact_no" length="25" type = "string"/>
	<property name="password" column="password" length="25" type = "string"/>
	<property name="registrationDate" column="reg_date" type = "timestamp"/>

</class>
</hibernate-mapping>